                </TableColumn>
                <TableColumn text="Resultado Visi">
                  <cellValueFactory><PropertyValueFactory property="resultado_Vi" />
                  </cellValueFactory>
                </TableColumn>
                <TableColumn text="Division">
                  <cellValueFactory><PropertyValueFactory property="division" />
                  </cellValueFactory>
                </TableColumn>
                <TableColumn text="date">
                  <cellValueFactory><PropertyValueFactory property="Fecha" />
                  </cellValueFactory>
                </TableColumn>
              </columns>
            </TableView>
            <Button fx:id="btAnadir" layoutX="47.0" layoutY="329.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="86.0" text="Añadir" />
         </children></AnchorPane>
