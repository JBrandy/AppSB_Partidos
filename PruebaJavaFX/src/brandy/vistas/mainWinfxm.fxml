            <TableView fx:id="tableViewPartidos" layoutX="17.0" layoutY="14.0" prefHeight="200.0" prefWidth="606.0">
              <columns>

                <TableColumn text="Local">
                  <cellValueFactory><PropertyValueFactory property="equipo_Local" />
